<form method="post" action="{{ path('unite_delete', {'id': unite.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cette unite?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ unite.id) }}">
    <button class="btn btn-danger">
        <i class="fa fa-trash"></i>
        Supprimer
    </button>
</form>
